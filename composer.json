{
  "name": "pragmatic/cookies-craftcms-plugin",
  "description": "Pragmatic Cookies extension for Pragmatic Web Toolkit",
  "type": "craft-plugin",
  "license": "MIT",
  "require": {
    "php": ">=8.2",
    "craftcms/cms": "^5.0",
    "pragmatic/web-toolkit-craftcms-plugin": "*"
  },
  "autoload": {
    "psr-4": {
      "pragmatic\\cookies\\": "src/"
    }
  },
  "authors": [
    {
      "name": "Oriol Noya",
      "email": "oriolnoya@pragmatic.cat"
    }
  ],
  "support": {
    "email": "oriolnoya@pragmatic.cat"
  },
  "extra": {
    "handle": "pragmatic-cookies",
    "class": "pragmatic\\cookies\\PragmaticCookies",
    "name": "Pragmatic Cookies",
    "developer": "Pragmatic",
    "developerUrl": "https://www.pragmatic.cat",
    "documentationUrl": "https://github.com/pragmaticcat/cookies-craftcms-plugin/blob/main/README.md",
    "hasSettings": false,
    "hasCpSection": false
  }
